LENDOSENSORES:
	MOV IN0,A;  //A=0000000P;
	MOV A,B;     //B=0000000P
	MOV IN1,A;  //A=0000000C;
	AND B,A;     //A = 0000000P e 0000000C
	MOV A,B;     //B = 0000000P e 0000000C
	MOV IN2,A;  //A = 0000000M
	NOT A,A;     //A = NEGACAO 0000000M
	MOV A,C;    //C  = NEGACAO 0000000M
	MOV IN3,A;  //A = 0000000E;
	OR C,A;       //A =  NEGACAO 0000000M ou 0000000E
	AND B,A;     // A=  RESPOSTA
	MOV A,OUT3; //EXIBE PARA O USUARIO
	JMP LENDOSENSORES;