LEITURA:
	MOV IN3,A;
	SUB 00,B;
	JMPZ OPERACAOAND;
	SUB 01,B;
	JMPZ OPERACAOOR;
	SUB 02,B;
	JMPZ OPERACAOXOR;
	SUB 03,B;
	JMPZ OPERACAOADD;
	JMP LEITURA;
OPERACAOAND:
	CALL VALORENTRADAS;
	AND D,A;  
	MOV A,OUT2;
	JMP LEITURA;
OPERACAOOR:	
	CALL VALORENTRADAS;
	OR D,A;
	MOV A,OUT2;
	JMP LEITURA;
OPERACAOXOR:
	CALL VALORENTRADAS;
	XOR D,A;
	MOV A,OUT2;
	JMP LEITURA;
OPERACAOADD:
	CALL VALORENTRADAS;
	ADD D,A;
	MOV A,OUT2;
	JMP LEITURA;

VALORENTRADAS:
	MOV IN1,A;
	MOV A,D;
	MOV IN2,A;
	RET;