LEITURA:
	MOV IN0,A; //0000000P
	MOV A,B;
	MOV IN1,A; //0000000C
	AND B,A;  // FAZ UM AND entre P e C
	MOV A,B;//   B = AND ENTRE P e C
	MOV IN2,A; //0000000M
	NOT A,A;     // NEGACAO DE 0000000M
	MOV A,C;    // C= NEGACAO DE 0000000M
	MOV IN3,A; // A=0000000E
	OR C,A;     // A = OR ENTRE /CINTO E EMBRAGEM
	AND B,A;   // A = A.B(/C+D)
	MOV A,OUT3;
	JMP LEITURA;